{
  "name": "marmoym-api",
  "version": "0.0.2",
  "description": "marmoym-api",
  "scripts": {
    "build": "./node_modules/.bin/gulp --gulpfile ./internals/gulp/gulpfile.js build",
    "lint": "./node_modules/.bin/tslint -t stylish -p ./tsconfig.json",
    "pm2:deploy": "pm2 deploy ./internals/pm2/ecosystem.config.js production",
    "pm2:start:prod": "yarn run build && pm2 startOrRestart ./internals/pm2/ecosystem.config.js --env production",
    "preinstall": "./internals/shell/preinstall.sh",
    "start:dev": "yarn run build && cross-env NODE_ENV=development node ./dist/app.js",
    "start:prod": "yarn run build && cross-env NODE_ENV=production node ./dist/app.js",
    "test": ""
  },
