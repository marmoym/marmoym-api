    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.11.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^10.3.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "chai": "^4.0.2",
    "cross-env": "^5.1.4",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-mocha": "^5.0.0",
    "gulp-sourcemaps": "^2.6.4",
